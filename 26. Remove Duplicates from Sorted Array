class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        
        int lastnum=INT_MIN;
        int k=0;
        for(int i=0;i<nums.size();i++)
        {
            if(nums[i]>lastnum)
            {
              nums[k]=nums[i];
              lastnum=nums[i];        
              k++;
            }
        }
        return k;
        
        
    }
};

//https://leetcode.com/problems/remove-duplicates-from-sorted-array/
